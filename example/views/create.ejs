<!DOCTYPE html>
<% var keys = Object.keys(result); %>

<html lang="en">
<head>
</head>
<body >
  <form method="POST" action="/api/$TABLE_NAME$/new"  style="margin: auto">
    <table>
      <tbody>
        <% for (var i = 0; i < keys.length; i++) { %>
          <tr>
          <td><label for="<%= keys[i] %>"><%= keys[i] %></label></td>
          <td><input name="<%= keys[i] %>" placeholder="<%= keys[i] %>"/></td>
          </tr>
          <% } %>
        <tr style="margin: auto; text-align: center">
          <td></td>
          <td>
            <input type="submit" value="Save"/>
          </td>
        </tr>
      </tbody>

    </table>

  </form>
</body>
</html>
