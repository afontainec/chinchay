<!DOCTYPE html>
<% var keys = Object.keys(result); %>

<html lang="en">
<head>
</head>
<body >
  <form method="POST" action="/api/$TABLE_NAME$/<%=result.id%>/edit"  style="margin: auto">
    <table>
      <tbody>
        <% for (var i = 0; i < keys.length; i++) { %>
        <% if((['id', 'created_at', 'updated_at']).indexOf(keys[i]) === -1) { %>
                <tr>
                <td><label for="<%= keys[i] %>"><%= keys[i] %></label></td>
                <td><input name="<%= keys[i] %>" value="<%= result[keys[i]] %>"/></td>
                </tr>
          <%  } %>
          <% } %>
        <tr style="margin: auto; text-align: center">
          <td></td>
          <td>
            <input type="submit" value="Update"/>
          </td>
        </tr>
      </tbody>

    </table>

  </form>
</body>
</html>
